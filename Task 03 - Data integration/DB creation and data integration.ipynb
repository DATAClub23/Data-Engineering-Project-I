      "    FOREIGN KEY (film_id) REFERENCES film(film_id)\n",
      ",\n",
      "    FOREIGN KEY (category_id) REFERENCES category(category_id)\n",
      "    );\n",
      "\"\"\"\n"
     ]
    }
   ],
