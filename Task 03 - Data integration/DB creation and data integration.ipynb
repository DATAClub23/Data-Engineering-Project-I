    "def connectMySQL():\n",
    "    try :\n",
    "        conn = connector.Connect(host=\"localhost\",    # your host, usually localhost\n",
    "                                         user=\"root\",         # your username\n",
    "                                         port=\"3306\"         # port (3306 default)\n",
    "                                         )      \n",
    "        # Creating the cursor\n",
    "        cur = conn.cursor()\n",
    "        print(\"Connected to MySQL\")\n",
    "        return conn,cur\n",
    "    \n",
    "    except connector.Error as e:\n",
    "        print(e)\n",
    "        return e\n",
    "        \n",
    "    \n",
    "\n",
    "connectMySQL()\n"
   ]
